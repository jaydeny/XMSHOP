@{
    Layout = "~/Views/Shared/_Form.cshtml";
}
<form id="form1">
    <div style="margin-top: 10px; margin-left: 10px; margin-right: 10px;">
        <div style="padding-top: 20px; margin-right: 30px;">
            <table class="form">
                <tr>
                    <th class="formTitle">订单编号</th>
                    <td class="formValue">
                        <input id="id" name="id" minlength="6" maxlength="16" type="text" class="form-control required" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">下单时间</th>
                    <td class="formValue">
                        <input id="order_date" name="order_date" minlength="6" maxlength="16" type="text" class="form-control required" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">会员账号</th>
                    <td class="formValue">
                        <input id="vip_AN" name="vip_AN" minlength="6" maxlength="16" type="text" class="form-control required" />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">联系方式</th>
                    <td class="formValue">
                        <input id="order_mp" name="order_mp" type="isMobile" class="form-control" required />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">订单总价</th>
                    <td class="formValue">
                        <input id="order_total" name="order_total" type="text" class="form-control" required />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">收货地址</th>
                    <td class="formValue">
                        <input id="address_name" name="address_name" type="text" class="form-control" required />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">商品名称</th>
                    <td class="formValue">
                        <input id="goods_name" name="goods_name" type="text" class="form-control" required />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">商品描述</th>
                    <td class="formValue">
                        <input id="goods_intro" name="goods_intro" type="text" class="form-control" required />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">购买数量</th>
                    <td class="formValue">
                        <input id="buy_count" name="buy_count" type="text" class="form-control" required />
                    </td>
                </tr>
                <tr>
                    <th class="formTitle">商品总价</th>
                    <td class="formValue">
                        <input id="buy_total" name="buy_total" type="text" class="form-control" required />
                    </td>
                </tr>
            </table>
        </div>
    </div>
</form>
<script src="~/Scripts/backEnd/Revenue@Form.js"></script>

