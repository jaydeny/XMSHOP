
<div class="registered-main">
    <div class="title">
        注册
    </div>
    <div class="form-group">
        <label for="name">账号</label>
        <input type="text" class="form-control" id="name" name="name" >
        <span class="warning" id="name_warning"></span>
    </div>
    <div class="form-group">
        <label for="pwd">密码</label>
        <input type="password" class="form-control" id="pwd" name="pwd" >
        <span class="warning" id="pwd_warning"></span>
    </div>
    <div class="form-group">
        <label for="confirm_pwd">确认密码</label>
        <input type="password" class="form-control" id="confirm_pwd" name="confirm_pwd" >
        <span class="warning" id="confirm_pwd_warning"></span>
    </div>
    <div class="form-group">
        <label for="email">邮箱</label>
        <input type="email" class="form-control" id="email" name="email" >
        <span class="warning" id="email_warning"></span>
    </div>
    <div class="form-group mar_b_20">
        <label for="tel">手机号</label>
        <input type="tel" class="form-control" id="tel" name="tel">
        <span class="warning" id="tel_warning"></span>
    </div>
    <button type="submit" class="btn btn-default registered_but" id="btnSignin">提交</button>
</div>
<script type="text/javascript" src="~/Scripts/vip/Vip@Signin.js"></script>

        
